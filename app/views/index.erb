<!-- <div class="container"> -->
  <div class="row">
    <aside class="col s12 m3">
      <div class="collection">
        <a id="index-btn" href="#" class="collection-item">Index</a>
        <a id="new-btn" href="#" class="collection-item">New</a>
        <%= erb :_new_field %>
      </div>
    </aside>
    <article class="col s12 m9">
      <div id="index-field">
        <ul class="collection">
          <% @contacts.each do |contact| %>
            <%= erb :_contact, locals: { contact: contact } %>
          <% end %>
        </ul>
      </div>
      <div id="search-field">
      </div>
    </article>
  </div>
